{% capture contentForQuerystring %}
  {{ content_for_header }}
{% endcapture %}
{% assign pageUrlWithHost = contentForQuerystring | split:'"pageurl":"' | last | split:'"' | first |
   replace:'\/','/' | 
   replace:'%20',' ' | 
   replace:'\u0026','&'
%}

{% assign queryParam = pageUrlWithHost | split: "?" | slice:1 %}
{% assign bypassLocks = false %}
{% unless queryParam == blank %}
  {% if queryParam contains 'bypassLocks=true' %}
    {% assign bypassLocks = true %}
  {% endif %}
{% endunless %}

{% assign exemptPages = false %}
{% if request.design_mode == true or template == 'customers/login' or template == 'customers/account' or template == 'customers/activate_account' or template == 'customers/reset_password' %}
